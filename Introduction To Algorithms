#include <stdio.h>

int main() {
    1. // Write the program that gives the sum of two numbers.
    int a,b,sum;
    printf("Enter first number:");
    scanf("%d",&a);
    printf("Enter second number:");
    scanf("%d",&b);
    sum=a+b;
    printf("The sum of two numbers is %d.",sum);
    return 0;
}


