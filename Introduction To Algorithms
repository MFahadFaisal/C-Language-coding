   int x,num,sum=0,digit;
   printf("Enter the number:");
   scanf("%d",&num);
   dnz:
   digit=num%10;
   sum=sum+digit^3;
   if(num<10)
   {
       sum=sum+num^3;
       goto dnz;
   }
   if(sum==num)
   
       printf("Equal");
    else
       printf("Not Equal");
  
    return 0;
}

#include <stdio.h>

int main() {
    //11. Write the program that finds the ratio of the sum of even numbers to the sum of odd numbers among 20 numbers entered from the keyboard.
    int even=0,odd=0,num,i;
    float ratio;
    for(i=1;i<=20;i++)
    {
    scanf("%d",&num);
    if(num%2==0)
    even=even+num;
    else
    odd=odd+num;
    }
    ratio=even/odd;
    printf("The ratio between even and odd numbers is %f",ratio);
    return 0;
}

